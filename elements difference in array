#include <stdio.h>

int main()
{
    int a[]={1000,4,5,6,722,11,55,77,999};
    int n=sizeof(a)/sizeof(a[0]);
    int diff=0;
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<n;j++)
        {
            if(a[i]-a[j]>diff)
            {
             diff=a[i]-a[j]; 
            }
        }
        
    }
 
        printf("%d",diff);
    
}
    
