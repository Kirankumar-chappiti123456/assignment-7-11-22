#include<stdio.h>
int a=10;
int main()
{
    extern int a;
    a++;
    printf("%d",a);
}

#include<stdio.h>
extern int a=10;
int main()
{
    a++;
    printf("%d",a);
}
#include<stdio.h>
int main()
{
  extern int a;
  extern int b;
  printf("%d\n",a);
  printf("%d",b);
}
